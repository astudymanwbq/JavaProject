<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbq.dao.LoginLogDao">
    <insert id="insertLoginLog" parameterType="com.wbq.dto.LoginLog">
        INSERT INTO login_log(account, login_date)
        VALUES (#{account},#{loginDate})
    </insert>
</mapper>